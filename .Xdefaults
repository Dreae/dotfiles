! Fonts
xft.dpi : 96
xft.rgba: rgb
xft.hinting: true
xft.hintstyle: hintslight

! URxvt
URxvt*font: xft:Hack:style=Regular:pixelsize=24
URxvt*letterSpace: -1
URxvt.scrollStyle: rxvt
URxvt.scrollBar: false
URxvt.cursorBlink: true
URxvt.cursorUnderline: false

! Thunar
Thunar*font: xft:Hack:style=Regular:pixelsize=24

! Colors
! Special
*.foreground:   #d3d4d5
*.background:   #272822
*.cursorColor:  #353c58

